{
  "name": "grayscaleartboards",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "grayscaleArtboards",
    "manifest": "src/manifest.json",
    "description":"A plugin to grayscale artboards",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rishbharUX/sketchPlugins.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Rishabh Bhardwaj <rishabh.bhardwaj1988@gmail.com>"
}
